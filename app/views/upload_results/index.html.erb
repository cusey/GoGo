<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>

<p id="notice"><%= notice %></p>

<h1>Upload Results</h1>

<table>
  <thead>
    <tr>
      <th>Upload Id</th>
      <th>Source</th>
      <th>Resource Id</th>
      <th>Is Valid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_results.each do |upload_result| %>
      <tr>
        <td><%= upload_result.uploadId %></td>
        <td><%= upload_result.source %></td>
        <td><%= upload_result.resourceId %></td>
        <td><%= upload_result.isValid %></td>
        <td><%= link_to 'Show', upload_result %></td>
        <td><%= link_to 'Edit', edit_upload_result_path(upload_result) %></td>
        <td><%= link_to 'Destroy', upload_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Home', root_path %> |
<%= link_to 'New Upload Result', new_upload_result_path %>
